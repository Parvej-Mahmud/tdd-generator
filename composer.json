{
    "name": "your-vendor/laravel-tdd-generator",
    "description": "Automatically generate comprehensive TDD files for Laravel modules including models, controllers, migrations, and routes",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "tdd", "test-driven-development", "testing", "artisan", "generator"],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\LaravelTddGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourVendor\\LaravelTddGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\LaravelTddGenerator\\TddGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}